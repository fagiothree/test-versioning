      ],
      "uuid": "083bf355-b5d4-495b-a455-9eaa728f53de"
    },
    {
      "actions": [
        {
          "all_urns": false,
          "attachments": [],
          "quick_replies": [],
          "text": "Please select another number if you want to learn more about how to get your children to follow instructions more often:\n\n1. Get their attention\n2. Be positive, specific, and realistic\n3. One instruction at a time\n4. Praise them for doing it! \n\nYou can also type \"N\" to stop this tip.",
          "type": "send_msg",
          "uuid": "abd789b8-47d7-4b74-9ea6-52da8eabde9e"
        }
      ],
      "exits": [
